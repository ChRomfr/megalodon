<!-- START app/view/adm/check_dir.meg -->
{strip}
<table class="table table-striped table-condensed">
	<thead>
		<tr>
			<th>Dossier</th>
			<th>Chemin</th>
			<th>Resultat</th>
		</tr>
	</thead>
	<tbody>
		{foreach $check_dir as $dir}
		<tr>
			<td>{$dir.name}</td>
			<td>{$dir.dir}</td>
			<td>{$dir.result}</td>
		</tr>
		{/foreach}
	</tbody>
</table>
{/strip}
<!-- END app/view/adm/check_dir.meg -->