<table class="table table-striped table-condensed">
	<thead>
		<tr>
			<th>Date</th>
			<th>Contacts</th>
			<th>Action</th>
		</tr>
	</thead>
	<tbody>
		{foreach $logs as $row}
		<tr>
			<td><small>{$row.date_log}</small></td>
			<td><small>{if !empty($row.raison_social)}{$row.raison_social}{else}{$row.nom}&nbsp;{$row.prenom}{/if}</small></td>
			<td><small>{$row.log}</small></td>
		</tr>
		{/foreach}
	</tbody>
</table>