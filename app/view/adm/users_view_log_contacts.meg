<table class="table table-striped table-condensed">
	<thead>
		<tr>
			<th>Date</th>
			<th>Modules</th>
			<th>Action</th>
		</tr>
	</thead>
	<tbody>
		{foreach $logs as $row}
		<tr>
			<td><small>{$row.date_log}</small></td>
			<td><small>{$row.module} #{$row.link_id}</small></td>
			<td><small>{$row.log}</small></td>
		</tr>
		{/foreach}
	</tbody>
</table>